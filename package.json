{
  "name": "fastfox",
  "version": "0.0.1",
  "description": "Node.js library for the FastFox API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "packageManager": "npm@10.4.0",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --fix --ext .ts ./src",
    "format:check": "prettier --check \"**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastfoxai/fastfoxai-node.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastfoxai/fastfoxai-node/issues"
  },
  "homepage": "https://github.com/fastfoxai/fastfoxai-node#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "18.19.15",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "jest-fetch-mock": "3.0.3",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsup": "7.2.0",
    "typescript": "5.3.3"
  }
}
